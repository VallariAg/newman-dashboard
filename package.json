    "@babel/plugin-proposal-decorators": "^7.14.5",
    "autoprefixer": "^10.3.1",
    "chai": "^4.3.4",
    "codecov": "^3.8.2",
    "editorconfig": "^0.15.3",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
